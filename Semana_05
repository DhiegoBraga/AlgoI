int main()
{
    int count, i;
    
    count = 0;
    while(count < 50 ){
        for (i=1; count < 50; i++ ) {
            if ((i%2) == 0) {
                printf("%d",i);
                printf("\n");
                count ++;
            } else {
            }
        }    
    }

    return 0;
}
